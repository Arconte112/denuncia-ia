{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "noEmit": true,
    "incremental": true,
    "types": ["node"],
    "jsx": "preserve",
    "typeRoots": ["./node_modules/@types"],
    "strictNullChecks": false,
    "strict": false,
    "allowJs": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
} 